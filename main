<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Run 3 (SWF) via GitHub Pages</title>

  <!-- Ruffle (Flash emulator) via CDN -->
  <script src="https://unpkg.com/@ruffle-rs/ruffle"></script>

  <style>
    html, body {
      margin: 0;
      height: 100%;
      background: #000;
      display: flex;
      justify-content: center;
      align-items: center;
    }
  </style>
</head>
<body>
  <div id="flash-container"></div>

  <script>
    const ruffle = window.RufflePlayer.newest();
    const player = ruffle.createPlayer();
    document.getElementById("flash-container").appendChild(player);

    // Load the SWF file
    player.load("Run3.swf");
  </script>
</body>
</html>
